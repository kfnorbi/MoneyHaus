<?xml version="1.0" encoding="UTF-8"?>
<mappings xmlns="http://dozer.sourceforge.net"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://dozer.sourceforge.net
          http://dozer.sourceforge.net/schema/beanmapping.xsd">
          
    <mapping map-id="currency-to-exchange">
        <class-a>com.github.sarxos.xchange.ExchangeRate;</class-a>
        <class-b>hu.unideb.inf.moneyhaus.vo.CurrencyRateVO</class-b>
        <field>
            <a>to</a>
            <b>base</b>
        </field>
        <field>
            <a>from</a>
            <b>currency</b>
        </field>
        <field>
            <a>rate</a>
            <b>rate</b>
        </field>
    </mapping>

</mappings> 